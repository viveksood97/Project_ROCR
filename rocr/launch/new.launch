<launch>
    <include file= "$(find gazebo_ros)/launch/empty_world.launch"> 
        <arg name="world_name" value="$(find rocr)/world/turtlebot3_house.world"/> 
        <arg name="paused" value="false"/>
    </include>

    <param name= "robot_description" command="$(find xacro)/xacro --inorder $(find rocr)/urdf/rocr_integration.urdf.xacro" /> 

    <!-- <param name= "robot_description" textfile="$(find rocr)/urdf/rocr.urdf" /> -->
    <param name = "my_robot" value="rocr"/> 
	<arg name ="init_pose" value="-x -1.5 -y 0.5 -z 0.08 -R 0 -P 0 -Y 0"/>

    <node
    name="spawn_model"
    pkg="gazebo_ros"
    type="spawn_model"
    args="$(arg init_pose) -param robot_description -urdf -model my_robot"
    output="screen"/>

    <rosparam file="$(find rocr)/config/diff_drive.yaml" command="load"/>

    <node name="controller_spwaner" pkg="controller_manager" type="spawner" respawn="false"
    	output="screen" args="rocr_controller" />  
    
</launch>